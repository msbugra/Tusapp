        "Onkoloji": ["kanser", "kemoterapi", "radyoterapi", "metastaz", "tÃ¼mÃ¶r", "malign"],
        
        // Genel Cerrahi
        "Meme HastalÄ±klarÄ±": ["meme", "fibroadenom", "karsinom", "mastektomi"],
        "Tiroid HastalÄ±klarÄ±": ["tiroid", "guatr", "hipertiroidi", "hipotiroidi"],
        "Ã–zefagus": ["Ã¶zofagus", "disfaji", "reflÃ¼", "akalazya"],
        "Mide": ["mide", "Ã¼lser", "gastrit", "gastrektomi"],
        "Kolorektal": ["kolon", "rektum", "kanser", "polip", "kolektomi"],
        "KaraciÄŸer": ["karaciÄŸer", "hepatit", "siroz", "hepatektomi"],
        "Safra Yolu ve Safra Kesesi": ["safra", "kolesistit", "kolanjit", "taÅŸ"],
        "Pankreas": ["pankreas", "pankreatit", "diabetes", "pankreatektomi"],
        "FÄ±tÄ±k": ["fÄ±tÄ±k", "herni", "inguinal", "umblikal"],
        
        // KadÄ±n HastalÄ±klarÄ± ve DoÄŸum
        "Jinekoloji": ["jinekologi", "menstruasyon", "amenore", "dismenore", "endometriozis"],
        "Obstetri": ["gebelik", "doÄŸum", "fetal", "plasenta", "preeklampsi"],
        "Onkoloji": ["over", "endometrium", "serviks", "karsinom"],
        "Endokrinoloji": ["hormon", "Ã¶strojen", "progesteron", "menopoz"]
    };
    
    // En iyi eÅŸleÅŸmeyi bul
    let bestMatch = "Genel";
    let maxScore = 0;
    
    Object.entries(keywordMappings).forEach(([subcategory, keywords]) => {
        if (!tusSubcategories[lesson].includes(subcategory)) return;
        
        let score = 0;
        keywords.forEach(keyword => {
            if (fullText.includes(keyword)) {
                score += keyword.length; // Uzun kelimeler daha spesifik
            }
        });
        
        if (score > maxScore) {
            maxScore = score;
            bestMatch = subcategory;
        }
    });
    
    return bestMatch;
}

// Alt konu analizini getir
function getSubcategoryAnalysis() {
    const analysis = {};
    
    questions.forEach(q => {
        const lesson = q.lesson;
        if (!analysis[lesson]) analysis[lesson] = {};
        
        const subcategory = categorizeQuestion(q, lesson);
        if (!analysis[lesson][subcategory]) analysis[lesson][subcategory] = 0;
        analysis[lesson][subcategory]++;
    });
    
    return analysis;
}

