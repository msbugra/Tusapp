let selectedLesson = lessons[0] || "";
let quizQuestions = [];
let currentQuestionIndex = 0;
let userAnswers = [];
let wrongQuestions = []; // YanlÄ±ÅŸ/boÅŸ sorularÄ± saklamak iÃ§in

// Performans takibi iÃ§in
let performanceData = getPerformanceData();

// --- UI ELEMENTS ---
const mainMenu = document.getElementById('main-menu');
const quizScreen = document.getElementById('quiz');
const lessonSelect = document.getElementById('lesson-select');
const mainMenuStatus = document.getElementById('main-menu-status');

// --- PERFORMANS TAKÄ°BÄ° ---
function getPerformanceData() {
    try {
        return JSON.parse(localStorage.getItem('performanceData') || '{}');
    } catch { return {}; }
}

function savePerformanceData() {
    localStorage.setItem('performanceData', JSON.stringify(performanceData));
}

function updatePerformance(lesson, isCorrect) {
    if (!performanceData[lesson]) {
        performanceData[lesson] = { correct: 0, wrong: 0, total: 0 };
    }
    performanceData[lesson].total++;
    if (isCorrect) {
        performanceData[lesson].correct++;
    } else {
        performanceData[lesson].wrong++;
    }
    savePerformanceData();
}

function getWeakLessons() {
    const weakLessons = [];
    Object.entries(performanceData).forEach(([lesson, data]) => {
        if (data.total >= 5) { // En az 5 soru Ã§Ã¶zÃ¼lmÃ¼ÅŸ olmalÄ±
            const successRate = data.correct / data.total;
            if (successRate < 0.6) { // %60'tan dÃ¼ÅŸÃ¼k baÅŸarÄ±
                weakLessons.push({
                    lesson,
                    successRate,
                    wrongCount: data.wrong
                });
            }
        }
    });
    return weakLessons.sort((a, b) => a.successRate - b.successRate);
}

// --- FAVORÄ° SORULAR ---
function getFavorites() {
    try {
        return JSON.parse(localStorage.getItem('favorites') || '[]');
    } catch { return []; }
}
function setFavorites(favs) {
    localStorage.setItem('favorites', JSON.stringify(favs));
}
function isFavorite(q) {
    const favs = getFavorites();
    return favs.some(f => f.lesson === q.lesson && f.question === q.question);
}
function toggleFavorite(q) {
    let favs = getFavorites();
    const idx = favs.findIndex(f => f.lesson === q.lesson && f.question === q.question);
    if (idx > -1) {
        favs.splice(idx, 1);
    } else {
        favs.push(q);
    }
    setFavorites(favs);
    showQuestion();
}
// --- UI & NAVIGATION ---
function showMainMenu() {
    mainMenu.classList.remove('hidden');
    quizScreen.classList.add('hidden');
    quizScreen.innerHTML = '';
    fillLessonSelects();
    // Arama kutusu ekle
let searchDiv = document.getElementById('search-div');
if (!searchDiv) {
    searchDiv = document.createElement('div');
    searchDiv.id = 'search-div';
    searchDiv.className = 'mb-4';
    searchDiv.innerHTML = `
        <input id="search-input" type="text" placeholder="Soru ara..." class="w-full p-3 rounded-md focus:outline-none focus:ring-2 mb-2" style="background-color: var(--bg-tertiary); border: 1px solid var(--border-color); color: var(--text-primary);" oninput="searchQuestions()">
        <div id="search-results" class="rounded-md p-2 max-h-64 overflow-y-auto text-sm" style="background-color: var(--bg-secondary); color: var(--text-primary);"></div>
    `;
    mainMenu.insertBefore(searchDiv, lessonSelect);
}
    // Ders aÃ§Ä±klama kutusu ekle
    let descDiv = document.getElementById('lesson-description');
    if (!descDiv) {
        descDiv = document.createElement('div');
        descDiv.id = 'lesson-description';
        descDiv.className = 'mb-4 p-3 rounded-md text-sm italic';
        descDiv.style.backgroundColor = 'var(--bg-tertiary)';
        descDiv.style.color = 'var(--text-muted)';
        descDiv.style.display = 'none';
        mainMenu.insertBefore(descDiv, mainMenu.querySelector('.space-y-4'));
    }
    updateLessonDescription();
    
    // Alt konu seÃ§imi ekle
    let subcatDiv = document.getElementById('subcategory-div');
    if (!subcatDiv) {
        subcatDiv = document.createElement('div');
        subcatDiv.id = 'subcategory-div';
        subcatDiv.className = 'mb-4';
        subcatDiv.innerHTML = `
            <label class="block text-sm font-medium mb-2" style="color: var(--text-secondary);">Alt Konu SeÃ§in:</label>
            <select id="subcategory-select" class="w-full p-3 rounded-md focus:outline-none focus:ring-2 mb-3" style="background-color: var(--bg-tertiary); border: 1px solid var(--border-color); color: var(--text-primary);">
                <option value="">TÃ¼m konular</option>
            </select>
            <button id="subcategory-quiz-btn" class="w-full font-bold py-3 px-6 rounded-md transition-colors duration-300" style="background-color: var(--accent-primary); color: #ffffff;" onclick="startSubcategoryQuiz()">Alt Konu Quiz BaÅŸlat</button>
        `;
        subcatDiv.style.display = 'none';
        mainMenu.insertBefore(subcatDiv, mainMenu.querySelector('.space-y-4'));
    }
    updateSubcategoryOptions();
    
    // Favori Sorular butonu ekle
    let favBtn = document.getElementById('fav-quiz-btn');
    if (!favBtn) {
        favBtn = document.createElement('button');
        favBtn.id = 'fav-quiz-btn';
        favBtn.className = 'w-full font-bold py-3 px-6 rounded-md transition-colors duration-300 mt-2';
        favBtn.style.backgroundColor = 'var(--warning-color)';
        favBtn.style.color = '#ffffff';
        favBtn.innerText = 'Favori Sorular';
        favBtn.onclick = startFavoriteQuiz;
        mainMenu.appendChild(favBtn);
    }
    // Sonra BakÄ±lanlar quiz butonu ekle
    let laterBtn = document.getElementById('later-quiz-btn');
    if (!laterBtn) {
        laterBtn = document.createElement('button');
        laterBtn.id = 'later-quiz-btn';
        laterBtn.className = 'w-full font-bold py-3 px-6 rounded-md transition-colors duration-300 mt-2';
        laterBtn.style.backgroundColor = '#8b5cf6';
        laterBtn.style.color = '#ffffff';
        laterBtn.innerText = 'Sonra BakÄ±lan Sorular';
        laterBtn.onclick = startLaterQuiz;
        mainMenu.appendChild(laterBtn);
    }
    // Karma Quiz butonu ekle
    let randomBtn = document.getElementById('random-quiz-btn');
    if (!randomBtn) {
        randomBtn = document.createElement('button');
        randomBtn.id = 'random-quiz-btn';
        randomBtn.className = 'w-full font-bold py-3 px-6 rounded-md transition-colors duration-300 mt-2';
        randomBtn.style.backgroundColor = 'var(--accent-secondary)';
        randomBtn.style.color = '#ffffff';
        randomBtn.innerText = 'Karma Quiz';
        randomBtn.onclick = startRandomQuiz;
        mainMenu.appendChild(randomBtn);
    }
    // AkÄ±llÄ± Ã–neri butonu ekle
    let smartBtn = document.getElementById('smart-quiz-btn');
    if (!smartBtn) {
        smartBtn = document.createElement('button');
        smartBtn.id = 'smart-quiz-btn';
        smartBtn.className = 'w-full font-bold py-3 px-6 rounded-md transition-colors duration-300 mt-2';
        smartBtn.style.backgroundColor = 'var(--success-color)';
        smartBtn.style.color = '#ffffff';
        smartBtn.innerHTML = 'ğŸ¤– AkÄ±llÄ± Ã–neri Quiz';
        smartBtn.onclick = startSmartQuiz;
        mainMenu.appendChild(smartBtn);
    }
    
    // Performans Ã¶zeti gÃ¶ster
    showPerformanceSummary();
    
    // Soru analizi gÃ¶ster
    showQuestionAnalysis();
    
    // Sonra bakÄ±lanlar gÃ¼ncellensin
    laterQuestions = getLaterQuestions();
}
function fillLessonSelects() {
    lessonSelect.innerHTML = '';
    // Temel TÄ±p Bilimleri
    const temelTip = [
        "Anatomi",
        "Fizyoloji, Histoloji ve Embriyoloji",
        "Fizyoloji",
        "Histoloji ve Embriyoloji",
        "TÄ±bbi Biyokimya",
        "Biyokimya",
        "TÄ±bbi Mikrobiyoloji",
        "Mikrobiyoloji",
        "TÄ±bbi Patoloji",
        "Patoloji",
        "TÄ±bbi Farmakoloji",
        "Farmakoloji"
    ];
    // Klinik Bilimler
    const klinik = [
        "Dahiliye",
        "Pediatri",
        "Genel Cerrahi",
        "KÃ¼Ã§Ã¼k Stajlar",
        "KadÄ±n HastalÄ±klarÄ± ve DoÄŸum"
    ];
    // Optgroup ile gruplandÄ±r
    let html = '';
    html += '<optgroup label="Temel TÄ±p Bilimleri">';
    temelTip.forEach(l => {
        if (lessons.includes(l)) {
            const count = questions.filter(q => q.lesson === l).length;
            html += `<option value="${l}">${l} (${count} soru)</option>`;
        }
    });
    html += '</optgroup>';
    html += '<optgroup label="Klinik Bilimler">';
    klinik.forEach(l => {
        if (lessons.includes(l)) {
            const count = questions.filter(q => q.lesson === l).length;
            html += `<option value="${l}">${l} (${count} soru)</option>`;
        }
    });
    html += '</optgroup>';
    lessonSelect.innerHTML = html;
    lessonSelect.value = selectedLesson;
}
lessonSelect.addEventListener('change', function() {
    selectedLesson = this.value;
    updateLessonDescription();
    updateSubcategoryOptions();
});

// Alt konu seÃ§imi gÃ¼ncelle
function updateSubcategoryOptions() {
    const subcatDiv = document.getElementById('subcategory-div');
    const subcatSelect = document.getElementById('subcategory-select');
    
    if (!subcatDiv || !subcatSelect) return;
    
    if (!selectedLesson || !tusSubcategories[selectedLesson]) {
        subcatDiv.style.display = 'none';
        return;
    }
    
    // Alt konularÄ± listele
    const subcategories = tusSubcategories[selectedLesson];
    let html = '<option value="">TÃ¼m konular</option>';
    
    // Bu dersteki sorulara gÃ¶re alt konu sayÄ±larÄ±nÄ± hesapla
    const lessonQuestions = questions.filter(q => q.lesson === selectedLesson);
    const subcatCounts = {};
    
    lessonQuestions.forEach(q => {
        const subcat = categorizeQuestion(q, selectedLesson);
        subcatCounts[subcat] = (subcatCounts[subcat] || 0) + 1;
    });
    
    subcategories.forEach(subcat => {
        const count = subcatCounts[subcat] || 0;
        if (count > 0) {
            html += `<option value="${subcat}">${subcat} (${count} soru)</option>`;
        }
    });
    
    subcatSelect.innerHTML = html;
    subcatDiv.style.display = 'block';
}

// Alt konu quiz baÅŸlat
window.startSubcategoryQuiz = function() {
    const subcatSelect = document.getElementById('subcategory-select');
    if (!subcatSelect) return;
    
    const selectedSubcategory = subcatSelect.value;
    if (!selectedSubcategory) {
        // TÃ¼m konular seÃ§iliyse normal quiz baÅŸlat
        startQuiz();
        return;
    }
    
    selectedLesson = lessonSelect.value;
    mainMenuStatus.textContent = '';
    
    // SeÃ§ili dersin seÃ§ili alt konusundaki sorularÄ± filtrele
    const lessonQuestions = questions.filter(q => q.lesson === selectedLesson);
    quizQuestions = lessonQuestions.filter(q => {
        const subcat = categorizeQuestion(q, selectedLesson);
        return subcat === selectedSubcategory;
    });
    
    if (quizQuestions.length === 0) {
        mainMenuStatus.textContent = `${selectedSubcategory} alt konusunda hiÃ§ soru bulunamadÄ±!`;
        setTimeout(() => { mainMenuStatus.textContent = ''; }, 3000);
        return;
    }
    
    if (quizQuestions.length < 3) {
        mainMenuStatus.textContent = `${selectedSubcategory} alt konusunda sadece ${quizQuestions.length} soru var.`;
        setTimeout(() => { mainMenuStatus.textContent = ''; }, 3000);
    }
    
    // Quiz baÅŸlat
    selectedLesson = `${selectedLesson} - ${selectedSubcategory}`;
    quizQuestions.sort(() => Math.random() - 0.5);
    currentQuestionIndex = 0;
    userAnswers = new Array(quizQuestions.length).fill(null);
    mainMenu.classList.add('hidden');
    quizScreen.classList.remove('hidden');
    showQuestion();
    startQuizTimer();
};

// Ders aÃ§Ä±klamasÄ±nÄ± gÃ¼ncelle
function updateLessonDescription() {
    const descDiv = document.getElementById('lesson-description');
    if (!descDiv) return;
    
    const desc = lessonDescriptions[selectedLesson];
    if (desc) {
        descDiv.innerHTML = `<strong>${selectedLesson}:</strong> ${desc}`;
        descDiv.style.display = 'block';
    } else {
        descDiv.style.display = 'none';
    }
}

// --- QUIZ LOGIC ---
function startQuiz() {
    selectedLesson = lessonSelect.value;
    mainMenuStatus.textContent = '';
    quizQuestions = questions.filter(q => q.lesson === selectedLesson);
    if (quizQuestions.length === 0) {
        mainMenuStatus.textContent = 'Bu derste hiÃ§ soru bulunamadÄ±!';
        setTimeout(() => { mainMenuStatus.textContent = ''; }, 3000);
        return;
    }
    if (quizQuestions.length < 5) {
        mainMenuStatus.textContent = `Bu derste sadece ${quizQuestions.length} soru var. Daha fazla soru eklenene kadar sÄ±nÄ±rlÄ± deneyim yaÅŸayabilirsiniz.`;
        setTimeout(() => { mainMenuStatus.textContent = ''; }, 4000);
    }
    // Shuffle questions for variety
    quizQuestions.sort(() => Math.random() - 0.5);
    currentQuestionIndex = 0;
    userAnswers = new Array(quizQuestions.length).fill(null);
    mainMenu.classList.add('hidden');
    quizScreen.classList.remove('hidden');
    showQuestion();
    startQuizTimer();
}
function startFavoriteQuiz() {
    selectedLesson = 'Favoriler';
    mainMenuStatus.textContent = '';
    quizQuestions = getFavorites();
    if (quizQuestions.length === 0) {
        mainMenuStatus.textContent = 'HiÃ§ favori sorunuz yok!';
        setTimeout(() => { mainMenuStatus.textContent = ''; }, 3000);
        return;
    }
    quizQuestions.sort(() => Math.random() - 0.5);
    currentQuestionIndex = 0;
    userAnswers = new Array(quizQuestions.length).fill(null);
    mainMenu.classList.add('hidden');
    quizScreen.classList.remove('hidden');
    showQuestion();
    startQuizTimer();
}
function showQuestion() {
    if (currentQuestionIndex >= quizQuestions.length) return;
    const q = quizQuestions[currentQuestionIndex];
    let favIcon = isFavorite(q) ? 'â˜…' : 'â˜†';
    let isLater = getLaterQuestions().some(item => item.lesson === q.lesson && item.question === q.question);
    // Ä°lerleme Ã§ubuÄŸu hesaplama
    const progress = Math.round(((currentQuestionIndex + 1) / quizQuestions.length) * 100);
    let progressBar = `
        <div class="w-full rounded-full h-3 mb-4" style="background-color: var(--bg-tertiary);">
            <div class="h-3 rounded-full transition-all duration-300" style="background-color: var(--accent-primary); width: ${progress}%"></div>
        </div>
        <div class="flex justify-between text-xs mb-2" style="color: var(--text-muted);">
            <span>Soru ${currentQuestionIndex + 1} / ${quizQuestions.length}</span>
            <span>%${progress} tamamlandÄ±</span>
        </div>
    `;
    let html = `
        <div id="quiz-timer" class="mb-4 text-right font-mono text-lg" style="color: var(--accent-primary);"></div>
        ${progressBar}
        <div class="flex justify-between items-center mb-2">
            <h2 class="text-xl font-semibold" style="color: var(--accent-primary);">${selectedLesson}</h2>
            <div class="flex items-center gap-2">
                <button onclick='toggleFavorite(quizQuestions[currentQuestionIndex])' title='Favorilere ekle/Ã§Ä±kar' class='text-yellow-400 text-2xl focus:outline-none'>${favIcon}</button>
                <button onclick='${isLater ? `unmarkLater(${currentQuestionIndex})` : `markLater()`}' title='Sonra bak' class='text-purple-400 text-xl focus:outline-none ml-2'>${isLater ? 'â³' : 'â­ï¸'}</button>
            </div>
        </div>
        <div class='text-lg mb-6'>${q.question}</div>
        <div id='options' class="space-y-3">
    `;
    const optionsKeys = Object.keys(q.options).sort();
    for (const key of optionsKeys) {
        html += `<div class='option rounded-md p-4 cursor-pointer border-2 border-transparent transition-all duration-200' data-key='${key}' onclick='selectOption("${key}")' style='background-color: var(--bg-tertiary); color: var(--text-primary); border-color: transparent;' onmouseover='this.style.borderColor="var(--accent-primary)"' onmouseout='this.style.borderColor="transparent"'>
            <span class="font-bold">${key})</span> ${q.options[key]}
        </div>`;
    }
    html += `</div>
        <div id='explanation' class='hidden mt-6 p-4 rounded-md' style='background-color: var(--bg-primary); border: 1px solid var(--accent-primary); color: var(--text-primary);'></div>
        <div class='mt-4'>
            ${getLaterQuestions().length ? '<div class="mb-2 flex flex-wrap gap-2"><span class="text-purple-300">Sonra bakÄ±lacaklar:</span>' + getLaterQuestions().map((item, idx) => `<button onclick='gotoLater(${idx})' class='bg-purple-700 hover:bg-purple-600 text-white rounded px-2 py-1 text-xs'>${idx+1}</button>`).join(' ') + '</div>' : ''}
        </div>
        <div class='mt-8 flex justify-between items-center space-x-4'>
            <button class='btn-nav' onclick='prevQ()' ${currentQuestionIndex === 0 ? 'disabled' : ''} style='background-color: var(--bg-tertiary); color: var(--text-primary); border: 1px solid var(--border-color);'>Geri</button>
            <button class='btn-nav' onclick='showMainMenu()' style='background-color: var(--bg-tertiary); color: var(--text-primary); border: 1px solid var(--border-color);'>Ana Sayfaya DÃ¶n</button>
            <button class='btn-nav' onclick='nextQ()' style='background-color: var(--accent-primary); color: #ffffff;'>${currentQuestionIndex === quizQuestions.length - 1 ? 'SonuÃ§larÄ± GÃ¶r' : 'Ä°leri'}</button>
        </div>
    `;
    quizScreen.innerHTML = html;
    quizScreen.querySelectorAll('.btn-nav').forEach(btn => {
        btn.classList.add('text-white', 'font-bold', 'py-2', 'px-4', 'rounded-md', 'transition-colors', 'duration-300', 'flex-1', 'disabled:bg-gray-700', 'disabled:cursor-not-allowed', 'disabled:opacity-50');
    });
    if (userAnswers[currentQuestionIndex] !== null) {
        showAnswered();
    }
    updateQuizTimer();
}
window.selectOption = function(key) {
    if (userAnswers[currentQuestionIndex] !== null) return; 
    userAnswers[currentQuestionIndex] = key;
    showAnswered();
};
function showAnswered() {
    const q = quizQuestions[currentQuestionIndex];
    const correctKey = q.correct;
    const selectedKey = userAnswers[currentQuestionIndex];
    document.querySelectorAll('.option').forEach(opt => {
        const key = opt.dataset.key;
        opt.style.pointerEvents = 'none';
        if (key === correctKey) {
            opt.style.backgroundColor = 'var(--success-color)';
            opt.style.borderColor = 'var(--success-color)';
            opt.style.color = '#ffffff';
        } else if (key === selectedKey) {
            opt.style.backgroundColor = 'var(--error-color)';
            opt.style.borderColor = 'var(--error-color)';
            opt.style.color = '#ffffff';
        } else {
            opt.style.opacity = '0.6';
        }
    });
    if (q.explanation) {
    const explanationDiv = document.getElementById('explanation');
        if (explanationDiv) {
            explanationDiv.classList.remove('hidden');
            explanationDiv.innerHTML = `<b style="color: var(--accent-primary);">AÃ§Ä±klama:</b> ${q.explanation}`;
        }
    }
}
window.prevQ = function() {
    if (currentQuestionIndex > 0) {
        currentQuestionIndex--;
        showQuestion();
    }
};
window.nextQ = function() {
    if (currentQuestionIndex < quizQuestions.length - 1) {
        currentQuestionIndex++;
        showQuestion();
    } else {
        showResult();
    }
};
function showResult() {
    stopQuizTimer();
    let score = 0;
    let wrongs = [];
    let blanks = [];
    let lessonStats = {};
    wrongQuestions = []; // YanlÄ±ÅŸ sorularÄ± temizle
    for (let i = 0; i < quizQuestions.length; i++) {
        const q = quizQuestions[i];
        const userAns = userAnswers[i];
        if (!lessonStats[q.lesson]) lessonStats[q.lesson] = { dogru: 0, yanlis: 0 };
        if (userAns === q.correct) {
            score++;
            lessonStats[q.lesson].dogru++;
            updatePerformance(q.lesson, true);
        } else {
            if (userAns === null) {
                blanks.push(i);
                wrongQuestions.push(q); // BoÅŸ soruyu ekle
            } else {
                wrongs.push(i);
                wrongQuestions.push(q); // YanlÄ±ÅŸ soruyu ekle
            }
            lessonStats[q.lesson].yanlis++;
            updatePerformance(q.lesson, false);
        }
    }
    const percentage = quizQuestions.length > 0 ? Math.round((score / quizQuestions.length) * 100) : 0;
    // En Ã§ok yanlÄ±ÅŸ yapÄ±lan dersi bul
    let maxWrong = 0, maxWrongLesson = null;
    Object.entries(lessonStats).forEach(([lesson, stat]) => {
        if (stat.yanlis > maxWrong) {
            maxWrong = stat.yanlis;
            maxWrongLesson = lesson;
        }
    });
    // YanlÄ±ÅŸ yapÄ±lan sorularÄ±n listesi
    let wrongListHtml = '';
    if (wrongs.length > 0) {
        wrongListHtml = `<div class=\"mt-6 text-left\"><b class='text-red-400'>YanlÄ±ÅŸ YapÄ±lan Sorular:</b><ul class='list-decimal ml-6 mt-2'>` +
            wrongs.map(i => `<li><b>${quizQuestions[i].lesson}:</b> ${quizQuestions[i].question}</li>`).join('') +
            '</ul></div>';
    }
    // Ders bazlÄ± Ã¶zet
    let lessonStatsHtml = '<div class=\"mt-6 text-left\"><b class=\"text-cyan-400\">Ders BazlÄ± DoÄŸru/YanlÄ±ÅŸ:</b><ul class=\"ml-6 mt-2\">';
    Object.entries(lessonStats).forEach(([lesson, stat]) => {
        lessonStatsHtml += `<li><b>${lesson}:</b> <span class='text-green-400'>${stat.dogru} doÄŸru</span>, <span class='text-red-400'>${stat.yanlis} yanlÄ±ÅŸ</span></li>`;
    });
    lessonStatsHtml += '</ul></div>';
    // En Ã§ok yanlÄ±ÅŸ yapÄ±lan ders
    let maxWrongHtml = '';
    if (maxWrongLesson) {
        maxWrongHtml = `<div class=\"mt-4 text-left\"><b class='text-yellow-400'>En Ã§ok yanlÄ±ÅŸ yapÄ±lan ders:</b> ${maxWrongLesson} (${maxWrong} yanlÄ±ÅŸ)</div>`;
    }
    let sureHtml = `<div class='mt-4 text-cyan-300'>Toplam SÃ¼re: <b>${formatTime(quizElapsed)}</b></div>`;
    
    // YanlÄ±ÅŸ veya boÅŸ soru varsa buton ekle
    let retryWrongBtn = '';
    if (wrongs.length > 0 || blanks.length > 0) {
        retryWrongBtn = `<button class='w-full bg-red-500 hover:bg-red-600 text-white font-bold py-3 px-6 rounded-md transition-colors duration-300' onclick='retryWrongQuestions()'>YanlÄ±ÅŸ/BoÅŸ SorularÄ± Tekrar Ã‡Ã¶z (${wrongs.length + blanks.length} soru)</button>`;
    }
    
    quizScreen.innerHTML = `
        <div class='text-center mt-6'>
            <h2 class="text-3xl font-bold text-cyan-400 mb-4">Quiz Bitti!</h2>
            <p class="text-xl mb-2">Skorunuz</p>
            <p class="text-5xl font-bold mb-6">${score} / ${quizQuestions.length}</p>
            <div class="w-full bg-gray-700 rounded-full h-4 mb-6">
                <div class="bg-green-500 h-4 rounded-full" style="width: ${percentage}%"></div>
            </div>
            <div class="space-y-4">
                <button class='w-full bg-cyan-500 hover:bg-cyan-600 text-white font-bold py-3 px-6 rounded-md transition-colors duration-300' onclick='restartQuiz()'>Tekrar Dene</button>
                ${retryWrongBtn}
                <button class='w-full bg-gray-500 hover:bg-gray-600 text-white font-bold py-3 px-6 rounded-md transition-colors duration-300' onclick='showMainMenu()'>Ana MenÃ¼</button>
                ${getLaterQuestions().length ? `<button class='w-full bg-purple-500 hover:bg-purple-600 text-white font-bold py-3 px-6 rounded-md transition-colors duration-300' onclick='startLaterQuiz()'>Sonra BakÄ±lan SorularÄ± Ã‡Ã¶z</button>` : ''}
                ${getLaterQuestions().length ? `<button class='w-full bg-gray-700 hover:bg-gray-800 text-white font-bold py-3 px-6 rounded-md transition-colors duration-300' onclick='clearLaterQuestions()'>Sonra BakÄ±lanlarÄ± Temizle</button>` : ''}
            </div>
            ${wrongListHtml}
            ${lessonStatsHtml}
            ${maxWrongHtml}
            ${sureHtml}
        </div>
    `;
}
window.clearLaterQuestions = function() {
    setLaterQuestions([]);
    laterQuestions = [];
    showMainMenu();
};
window.restartQuiz = function() {
    startQuiz();
};

// YanlÄ±ÅŸ sorularÄ± tekrar Ã§Ã¶z
window.retryWrongQuestions = function() {
    if (wrongQuestions.length === 0) return;
    
    selectedLesson = 'YanlÄ±ÅŸlar';
    quizQuestions = wrongQuestions.slice(); // KopyasÄ±nÄ± al
    quizQuestions.sort(() => Math.random() - 0.5); // KarÄ±ÅŸtÄ±r
    currentQuestionIndex = 0;
    userAnswers = new Array(quizQuestions.length).fill(null);
    wrongQuestions = []; // Temizle
    
    mainMenu.classList.add('hidden');
    quizScreen.classList.remove('hidden');
    showQuestion();
    startQuizTimer();
};

// --- SONRA BAKILACAK SORULAR ---
function getLaterQuestions() {
    try {
        return JSON.parse(localStorage.getItem('laterQuestions') || '[]');
    } catch { return []; }
}
function setLaterQuestions(arr) {
    localStorage.setItem('laterQuestions', JSON.stringify(arr));
}
let laterQuestions = getLaterQuestions();
function markLater() {
    const q = quizQuestions[currentQuestionIndex];
    // Soru objesini kaydet (tekrar eklenmesin)
    let arr = getLaterQuestions();
    if (!arr.some(item => item.lesson === q.lesson && item.question === q.question)) {
        arr.push(q);
        setLaterQuestions(arr);
        laterQuestions = arr;
    }
    showQuestion();
}
function unmarkLater(idx) {
    let arr = getLaterQuestions();
    const q = quizQuestions[idx];
    arr = arr.filter(item => !(item.lesson === q.lesson && item.question === q.question));
    setLaterQuestions(arr);
    laterQuestions = arr;
    showQuestion();
}
function gotoLater(idx) {
    // Soruya gitmek iÃ§in quizQuestions'da index bul
    const q = laterQuestions[idx];
    const realIdx = quizQuestions.findIndex(qq => qq.lesson === q.lesson && qq.question === q.question);
    if (realIdx !== -1) {
        currentQuestionIndex = realIdx;
        showQuestion();
    }
}
function startLaterQuiz() {
    let arr = getLaterQuestions();
    if (!arr.length) return;
    quizQuestions = arr.slice();
    currentQuestionIndex = 0;
    userAnswers = new Array(quizQuestions.length).fill(null);
    // Quiz baÅŸlarken sonra bakÄ±lanlar temizlenmez!
    mainMenu.classList.add('hidden');
    quizScreen.classList.remove('hidden');
    showQuestion();
    startQuizTimer();
}
function startRandomQuiz() {
    selectedLesson = 'Karma';
    mainMenuStatus.textContent = '';
    // TÃ¼m sorulardan rastgele 20 soru seÃ§
    let allQs = questions.slice();
    allQs.sort(() => Math.random() - 0.5);
    quizQuestions = allQs.slice(0, 20); // 20 soru, istenirse deÄŸiÅŸtirilebilir
    if (quizQuestions.length === 0) {
        mainMenuStatus.textContent = 'HiÃ§ soru bulunamadÄ±!';
        setTimeout(() => { mainMenuStatus.textContent = ''; }, 3000);
        return;
    }
    currentQuestionIndex = 0;
    userAnswers = new Array(quizQuestions.length).fill(null);
    mainMenu.classList.add('hidden');
    quizScreen.classList.remove('hidden');
    showQuestion();
    startQuizTimer();
}

// Performans Ã¶zeti gÃ¶ster
function showPerformanceSummary() {
    const weakLessons = getWeakLessons();
    let summaryDiv = document.getElementById('performance-summary');
    
    if (!summaryDiv) {
        summaryDiv = document.createElement('div');
        summaryDiv.id = 'performance-summary';
        summaryDiv.className = 'mt-4 p-4 rounded-md text-sm';
        summaryDiv.style.backgroundColor = 'var(--bg-secondary)';
        summaryDiv.style.borderColor = 'var(--border-color)';
        summaryDiv.style.border = '1px solid var(--border-color)';
        mainMenu.appendChild(summaryDiv);
    }
    
    if (weakLessons.length > 0) {
        let html = '<h3 class="font-bold mb-2" style="color: var(--warning-color);">ğŸ“Š GeliÅŸtirilmesi Gereken Dersler:</h3><ul class="space-y-1">';
        weakLessons.slice(0, 3).forEach(w => {
            const percent = Math.round(w.successRate * 100);
            html += `<li style="color: var(--text-secondary);">â€¢ <b>${w.lesson}</b>: %${percent} baÅŸarÄ± (${w.wrongCount} yanlÄ±ÅŸ)</li>`;
        });
        html += '</ul>';
        summaryDiv.innerHTML = html;
        summaryDiv.style.display = 'block';
        } else {
        summaryDiv.style.display = 'none';
    }
}

// Soru analizi gÃ¶ster
function showQuestionAnalysis() {
    let analysisDiv = document.getElementById('question-analysis');
    
    if (!analysisDiv) {
        analysisDiv = document.createElement('div');
        analysisDiv.id = 'question-analysis';
        analysisDiv.className = 'mt-4 p-4 rounded-md text-sm';
        analysisDiv.style.backgroundColor = 'var(--bg-secondary)';
        analysisDiv.style.borderColor = 'var(--accent-primary)';
        analysisDiv.style.border = '1px solid var(--accent-primary)';
        mainMenu.appendChild(analysisDiv);
    }
    
    // Ders bazlÄ± soru sayÄ±larÄ±
    const lessonCounts = {};
    questions.forEach(q => {
        lessonCounts[q.lesson] = (lessonCounts[q.lesson] || 0) + 1;
    });
    
    // Alt konu analizi
    const subcategoryAnalysis = getSubcategoryAnalysis();
    
    // En fazla ve en az sorulu dersler
    const sortedLessons = Object.entries(lessonCounts).sort((a, b) => b[1] - a[1]);
    const maxQuestions = sortedLessons[0];
    const minQuestions = sortedLessons[sortedLessons.length - 1];
    
    // Az sorulu dersler (5'ten az)
    const lowQuestionLessons = sortedLessons.filter(([lesson, count]) => count < 5);
    
    // Toplam istatistikler
    const totalQuestions = questions.length;
    const totalLessons = Object.keys(lessonCounts).length;
    const avgQuestionsPerLesson = Math.round(totalQuestions / totalLessons);
    
    // Son eklenen sorular analizi - en Ã§ok sorulu dersler muhtemelen son gÃ¼ncellenmiÅŸ
    const topLessons = sortedLessons.slice(0, 3);
    let recentAdditionsHtml = '';
    
    // Pediatri ve KadÄ±n DoÄŸum Ã¶zel olarak vurgula (son eklenenler)
    const specialLessons = ['Pediatri', 'KadÄ±n HastalÄ±klarÄ± ve DoÄŸum'];
    specialLessons.forEach(lesson => {
        if (lessonCounts[lesson]) {
            recentAdditionsHtml += `<li style="color: var(--success-color);">â€¢ <b>${lesson}</b>: ${lessonCounts[lesson]} soru âœ¨ (Son gÃ¼ncellemeler)</li>`;
        }
    });
    
    // Ã‡ok sorulu diÄŸer dersler
    topLessons.forEach(([lesson, count]) => {
        if (!specialLessons.includes(lesson) && count > 15) {
            recentAdditionsHtml += `<li style="color: var(--accent-secondary);">â€¢ <b>${lesson}</b>: ${count} soru (Zengin iÃ§erik)</li>`;
        }
    });
    
    let html = `
        <h3 class="font-bold mb-3" style="color: var(--accent-primary);">ğŸ“ˆ Soru BankasÄ± Analizi</h3>
        <div class="grid grid-cols-1 md:grid-cols-2 gap-4 mb-3">
            <div>
                <h4 class="font-semibold mb-1" style="color: var(--success-color);">ğŸ“Š Genel Ä°statistikler:</h4>
                <ul class="space-y-1" style="color: var(--text-secondary);">
                    <li>â€¢ Toplam Soru: <b style="color: var(--accent-primary);">${totalQuestions}</b></li>
                    <li>â€¢ Toplam Ders: <b style="color: var(--accent-primary);">${totalLessons}</b></li>
                    <li>â€¢ Ortalama Soru/Ders: <b style="color: var(--accent-primary);">${avgQuestionsPerLesson}</b></li>
                </ul>
            </div>
            <div>
                <h4 class="font-semibold mb-1" style="color: var(--accent-secondary);">ğŸ† En Zengin Dersler:</h4>
                <ul class="space-y-1" style="color: var(--text-secondary);">
                    ${sortedLessons.slice(0, 3).map(([lesson, count]) => 
                        `<li>â€¢ <b>${lesson}</b>: ${count} soru</li>`
                    ).join('')}
                </ul>
            </div>
        </div>
    `;
    
    if (recentAdditionsHtml) {
        html += `
            <div class="mb-3">
                <h4 class="font-semibold mb-1" style="color: var(--success-color);">âœ¨ Ã–ne Ã‡Ä±kan Dersler:</h4>
                <ul class="space-y-1">
                    ${recentAdditionsHtml}
                </ul>
            </div>
        `;
    }
    
    // Konu analizi - hangi konularda sorular var
    const topicAnalysis = analyzeQuestionTopics();
    if (topicAnalysis.length > 0) {
        html += `
            <div class="mb-3">
                <h4 class="font-semibold mb-1" style="color: var(--warning-color);">ğŸ¯ Konu DaÄŸÄ±lÄ±mÄ±:</h4>
                <div class="text-xs mb-2" style="color: var(--text-muted);">Son eklenen sorulardaki ana konular:</div>
                <div class="flex flex-wrap gap-1">
                    ${topicAnalysis.map(topic => 
                        `<span class="px-2 py-1 rounded text-xs" style="background-color: var(--bg-tertiary); color: var(--accent-primary);">${topic}</span>`
                    ).join('')}
                </div>
            </div>
        `;
    }
    
    // TUS Alt Konu DaÄŸÄ±lÄ±mÄ±
    html += `
        <div class="mb-3">
            <h4 class="font-semibold mb-2" style="color: var(--warning-color);">ğŸ¯ TUS Alt Konu DaÄŸÄ±lÄ±mÄ±:</h4>
            <div class="text-xs mb-2" style="color: var(--text-muted);">Sorular TUS mÃ¼fredatÄ±na gÃ¶re alt konulara kategorize edilmiÅŸtir:</div>
    `;
    
    // Her ders iÃ§in alt konu daÄŸÄ±lÄ±mÄ±nÄ± gÃ¶ster
    Object.entries(subcategoryAnalysis).forEach(([lesson, subcats]) => {
        const sortedSubcats = Object.entries(subcats).sort(([,a], [,b]) => b - a);
        if (sortedSubcats.length > 0) {
            html += `
                <div class="mb-2 p-2 rounded" style="background-color: var(--bg-tertiary);">
                    <div class="font-medium mb-1" style="color: var(--accent-primary);">${lesson} (${lessonCounts[lesson]} soru):</div>
                    <div class="text-xs grid grid-cols-2 gap-1">
                        ${sortedSubcats.map(([subcat, count]) => 
                            `<span style="color: var(--text-secondary);">â€¢ ${subcat}: <b style="color: var(--accent-secondary);">${count}</b></span>`
                        ).join('')}
                    </div>
                </div>
            `;
        }
    });
    
    html += `</div>`;
    
    if (lowQuestionLessons.length > 0) {
        html += `
            <div class="mb-3">
                <h4 class="font-semibold mb-1" style="color: var(--error-color);">âš ï¸ GeliÅŸtirilmesi Gereken Dersler:</h4>
                <ul class="space-y-1" style="color: var(--text-secondary);">
                    ${lowQuestionLessons.map(([lesson, count]) => 
                        `<li>â€¢ <b>${lesson}</b>: ${count} soru (Daha fazla soru gerekli)</li>`
                    ).join('')}
                </ul>
            </div>
        `;
    }
    
    // Kategori bazlÄ± analiz
    const temelTipSorular = sortedLessons.filter(([lesson]) => 
        ['Anatomi', 'Fizyoloji, Histoloji ve Embriyoloji', 'Biyokimya', 'Mikrobiyoloji', 'Patoloji', 'Farmakoloji'].includes(lesson)
    ).reduce((sum, [, count]) => sum + count, 0);
    
    const klinikSorular = sortedLessons.filter(([lesson]) => 
        ['Dahiliye', 'Pediatri', 'Genel Cerrahi', 'KÃ¼Ã§Ã¼k Stajlar', 'KadÄ±n HastalÄ±klarÄ± ve DoÄŸum'].includes(lesson)
    ).reduce((sum, [, count]) => sum + count, 0);
    
    html += `
        <div class="border-t pt-3" style="border-color: var(--border-color);">
            <h4 class="font-semibold mb-1" style="color: #8b5cf6;">ğŸ¯ Kategori DaÄŸÄ±lÄ±mÄ±:</h4>
            <div class="flex justify-between" style="color: var(--text-secondary);">
                <span>Temel TÄ±p: <b style="color: var(--accent-secondary);">${temelTipSorular}</b> soru</span>
                <span>Klinik: <b style="color: var(--success-color);">${klinikSorular}</b> soru</span>
            </div>
            <div class="w-full rounded-full h-2 mt-2" style="background-color: var(--bg-tertiary);">
                <div class="h-2 rounded-l-full" style="background-color: var(--accent-secondary); width: ${(temelTipSorular / totalQuestions) * 100}%"></div>
                <div class="h-2 rounded-r-full" style="background-color: var(--success-color); width: ${(klinikSorular / totalQuestions) * 100}%; margin-top: -8px; margin-left: ${(temelTipSorular / totalQuestions) * 100}%"></div>
            </div>
        </div>
    `;
    
    analysisDiv.innerHTML = html;
}

// Konu analizi - sorulardaki ana konularÄ± tespit et
function analyzeQuestionTopics() {
    const topics = [];
    const topicKeywords = {
        'Neonatoloji': ['yenidoÄŸan', 'neonatal', 'bebek', 'doÄŸum', 'gestasyon'],
        'Kardiyoloji': ['kalp', 'kardiyak', 'Ã¼fÃ¼rÃ¼m', 'kardiyomiyopati', 'EKG'],
        'Gastroenteroloji': ['ishal', 'karÄ±n', 'baÄŸÄ±rsak', 'mide', 'gastro'],
        'Endokrinoloji': ['diyabet', 'tiroid', 'hormon', 'endokrin'],
        'Genetik': ['sendrom', 'kromozom', 'kalÄ±tÄ±m', 'genetik', 'mutasyon'],
        'Ä°mmÃ¼noloji': ['immÃ¼n', 'enfeksiyon', 'aÅŸÄ±', 'antikor'],
        'Onkoloji': ['kanser', 'tÃ¼mÃ¶r', 'lenfoma', 'kemoterapi'],
        'NÃ¶roloji': ['sinir', 'nÃ¶ro', 'geliÅŸim', 'motor', 'miyotonik'],
        'Hematoloji': ['kan', 'anemi', 'polisitemi', 'tromboz'],
        'Farmakoloji': ['ilaÃ§', 'tedavi', 'yan etki', 'doz']
    };
    
    // Son 30 soruya bak (yakÄ±n zamanda eklenenler)
    const recentQuestions = questions.slice(-30);
    
    Object.entries(topicKeywords).forEach(([topic, keywords]) => {
        const found = recentQuestions.some(q => 
            keywords.some(keyword => 
                q.question.toLowerCase().includes(keyword) || 
                (q.explanation && q.explanation.toLowerCase().includes(keyword))
            )
        );
        if (found) topics.push(topic);
    });
    
    return topics;
}

// AkÄ±llÄ± Ã¶neri quiz baÅŸlat
function startSmartQuiz() {
    selectedLesson = 'AkÄ±llÄ± Ã–neri';
    mainMenuStatus.textContent = '';
    
    const weakLessons = getWeakLessons();
    if (weakLessons.length === 0) {
        mainMenuStatus.textContent = 'HenÃ¼z yeterli veri yok! En az 5 soru Ã§Ã¶zÃ¼lmÃ¼ÅŸ derslerde %60\'tan dÃ¼ÅŸÃ¼k baÅŸarÄ± aranÄ±yor.';
        setTimeout(() => { mainMenuStatus.textContent = ''; }, 4000);
        return;
    }
    
    // ZayÄ±f derslerden aÄŸÄ±rlÄ±klÄ± soru seÃ§
    quizQuestions = [];
    const questionPool = [];
    
    // Her zayÄ±f dersten sorular ekle (baÅŸarÄ± oranÄ±na gÃ¶re aÄŸÄ±rlÄ±klÄ±)
    weakLessons.forEach(w => {
        const lessQs = questions.filter(q => q.lesson === w.lesson);
        const weight = Math.ceil((1 - w.successRate) * 10); // Daha dÃ¼ÅŸÃ¼k baÅŸarÄ± = daha fazla soru
        for (let i = 0; i < weight; i++) {
            questionPool.push(...lessQs);
        }
    });
    
    // KarÄ±ÅŸtÄ±r ve 20 soru seÃ§
    questionPool.sort(() => Math.random() - 0.5);
    quizQuestions = questionPool.slice(0, 20);
    
    if (quizQuestions.length === 0) {
        mainMenuStatus.textContent = 'ZayÄ±f derslerde soru bulunamadÄ±!';
        setTimeout(() => { mainMenuStatus.textContent = ''; }, 3000);
        return;
    }
    
    currentQuestionIndex = 0;
    userAnswers = new Array(quizQuestions.length).fill(null);
    mainMenu.classList.add('hidden');
    quizScreen.classList.remove('hidden');
    showQuestion();
    startQuizTimer();
}

window.searchQuestions = function() {
    const val = (document.getElementById('search-input')?.value || '').toLowerCase();
    const resultsDiv = document.getElementById('search-results');
    if (!val || val.length < 2) {
        resultsDiv.innerHTML = '';
        return;
    }
    let results = questions.filter(q =>
        q.question.toLowerCase().includes(val) ||
        (q.explanation && q.explanation.toLowerCase().includes(val)) ||
        Object.values(q.options).some(opt => opt.toLowerCase().includes(val))
    );
    if (results.length === 0) {
        resultsDiv.innerHTML = '<div class="text-gray-400">SonuÃ§ bulunamadÄ±.</div>';
        return;
    }
    resultsDiv.innerHTML = results.map(q =>
    `<div class='mb-2 p-2 bg-gray-700 rounded cursor-pointer hover:bg-gray-600'
         data-lesson="${q.lesson}"
         data-question="${encodeURIComponent(q.question)}">
         <b>${q.lesson}</b>: ${q.question}
     </div>`
).join('');
/* -- SonuÃ§ satÄ±rlarÄ±nÄ± tÄ±klanabilir yap -- */
resultsDiv.querySelectorAll('div[data-lesson]').forEach(div => {
    div.addEventListener('click', () => {
        const lesson = div.getAttribute('data-lesson');
        const questionText = decodeURIComponent(div.getAttribute('data-question'));
        openSearchResult(lesson, questionText);
    });
});
};
/* Arama sonucuna tÄ±klandÄ±ÄŸÄ±nda ilgili dersi yÃ¼kleyip soruyu aÃ§ */
function openSearchResult(lesson, questionText) {
    selectedLesson = lesson;
    quizQuestions = questions.filter(q => q.lesson === lesson);
    currentQuestionIndex = quizQuestions.findIndex(q => q.question === questionText);
    if (currentQuestionIndex === -1) currentQuestionIndex = 0;   // GÃ¼venlik
    userAnswers = new Array(quizQuestions.length).fill(null);
    mainMenu.classList.add('hidden');
    quizScreen.classList.remove('hidden');
    showQuestion();
}
// Tema baÅŸlatma ve event listener
function initializeTheme() {
    applyTheme(currentTheme);
    document.getElementById('theme-toggle').addEventListener('click', toggleTheme);
}

function toggleTheme() {
    currentTheme = currentTheme === 'dark' ? 'light' : 'dark';
    applyTheme(currentTheme);
    localStorage.setItem('theme', currentTheme);
}

function applyTheme(theme) {
    const body = document.body;
    const themeIcon = document.getElementById('theme-icon');
    
    if (theme === 'light') {
        body.setAttribute('data-theme', 'light');
        themeIcon.textContent = 'â˜€ï¸';
    } else {
        body.removeAttribute('data-theme');
        themeIcon.textContent = 'ğŸŒ™';
    }
    
    // Dinamik elementleri gÃ¼ncelle
    updateDynamicElementStyles();
}

function updateDynamicElementStyles() {
    // Arama kutusu
    const searchInput = document.getElementById('search-input');
    if (searchInput) {
        searchInput.style.backgroundColor = 'var(--bg-tertiary)';
        searchInput.style.borderColor = 'var(--border-color)';
        searchInput.style.color = 'var(--text-primary)';
    }
    
    // Arama sonuÃ§larÄ±
    const searchResults = document.getElementById('search-results');
    if (searchResults) {
        searchResults.style.backgroundColor = 'var(--bg-secondary)';
        searchResults.style.color = 'var(--text-primary)';
    }
    
    // Ders aÃ§Ä±klamasÄ±
    const lessonDesc = document.getElementById('lesson-description');
    if (lessonDesc) {
        lessonDesc.style.backgroundColor = 'var(--bg-tertiary)';
        lessonDesc.style.color = 'var(--text-muted)';
    }
    
    // Alt konu seÃ§imi
    const subcatSelect = document.getElementById('subcategory-select');
    if (subcatSelect) {
        subcatSelect.style.backgroundColor = 'var(--bg-tertiary)';
        subcatSelect.style.borderColor = 'var(--border-color)';
        subcatSelect.style.color = 'var(--text-primary)';
    }
    
    // Performans ve analiz panelleri
    const panels = ['performance-summary', 'question-analysis'];
    panels.forEach(id => {
        const panel = document.getElementById(id);
        if (panel) {
            panel.style.backgroundColor = 'var(--bg-secondary)';
            panel.style.borderColor = 'var(--border-color)';
            panel.style.color = 'var(--text-primary)';
        }
    });
}

document.addEventListener('DOMContentLoaded', () => {
    initializeTheme();
    showMainMenu();
});

if (document.readyState !== 'loading') {
    initializeTheme();
    showMainMenu();
}

// Quiz zamanlayÄ±cÄ± baÅŸlatma
function startQuizTimer() {
    if (quizTimer) clearInterval(quizTimer);
    quizStartTime = Date.now();
    quizElapsed = 0;
    updateQuizTimer();
    quizTimer = setInterval(() => {
        quizElapsed = Math.floor((Date.now() - quizStartTime) / 1000);
        updateQuizTimer();
    }, 1000);
}
// Quiz zamanlayÄ±cÄ±yÄ± durdur
function stopQuizTimer() {
    if (quizTimer) clearInterval(quizTimer);
    quizTimer = null;
}
// ZamanlayÄ±cÄ±yÄ± ekrana yaz
function updateQuizTimer() {
    const timerDiv = document.getElementById('quiz-timer');
    if (timerDiv) {
        timerDiv.textContent = 'SÃ¼re: ' + formatTime(quizElapsed);
    }
}
function formatTime(sec) {
    const m = Math.floor(sec / 60);
    const s = sec % 60;
    return m + ':' + (s < 10 ? '0' : '') + s;
}
</script>
</body>
</html>
